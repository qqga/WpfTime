<Window x:Class="WpfTime.WindowFormat"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfTime"
        mc:Ignorable="d"
        Title="WindowFormat" Height="82.609" Width="115.217">
    <StackPanel>
        <TextBox Name="TextBoxFormat" Text="HH:mm:ss"></TextBox>
        <StackPanel Orientation="Horizontal">
        <Button x:Name="buttonOk" Click="buttonOk_Click" Content="Ok"  Margin="4"/>
        <Button x:Name="buttonCancel" Click="buttonCancel_Click" Content="Cancel" Margin="4"/>
        </StackPanel>
    </StackPanel>
</Window>
